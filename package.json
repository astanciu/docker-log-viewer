{
  "name": "docker-log-viewer",
  "version": "0.0.1",
  "main": "app/main/main.js",
  "license": "MIT",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dev": "concurrently -r -k -n webpack,electron \"yarn bundle\" \"yarn start\"",
    "bundle": "NODE_ENV=development webpack --mode development --config config/webpack.dev.js",
    "build": "NODE_ENV=production webpack --mode production --config config/webpack.prod.js",
    "dist": "yarn build && rm -rf dist && electron-builder"
  },
  "dependencies": {
    "bluebird": "^3.5.3",
    "dockerode": "^2.5.7",
    "lodash": "^4.17.11",
    "node-sass": "^4.11.0",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "react-virtualized": "^9.21.0",
    "sass-loader": "^7.1.0",
    "semantic-ui-css": "^2.4.1",
    "semantic-ui-react": "^0.84.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^1.0.0",
    "concurrently": "^4.1.0",
    "css-loader": "^2.1.0",
    "electron": "^4.0.0",
    "electron-builder": "^20.38.4",
    "electron-reload": "^1.4.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^3.0.1",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "html-webpack-root-plugin": "^0.10.0",
    "mini-css-extract-plugin": "^0.5.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.1.2"
  },
  "build": {
    "appId": "com.astanciu.DockerLogViewer",
    "productName": "Docker Log Viewer",
    "mac": {
      "category": "public.app-category.developer-tools",
      "icon": "assets/icon.icns",
      "identity": null,
      "target": "zip"
    },
    "directories": {
      "buildResources": "assets",
      "output": "dist"
    }
  }
}
